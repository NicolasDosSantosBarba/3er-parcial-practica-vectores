#include <iostream>
#include <vector>
using namespace std;

int main() {
    int n;
    cin >> n;

    vector<double> v(n);
    for (int i = 0; i < n; i++) cin >> v[i];

    int i = 0, j = n - 1;

    while (i < j) {
        double aux = v[i];
        v[i] = v[j];
        v[j] = aux;
        i++; j--;
    }

    for (double x : v) cout << x << " ";
}
