#include <iostream>
#include <vector>
using namespace std;

int main() {
    int n;
    cin >> n;

    vector<string> nom(n);
    vector<double> nota(n);

    for (int i = 0; i < n; i++) {
        cin >> nom[i];
        cin >> nota[i];
    }

    for (int i = 0; i < n - 1; i++) {
        for (int j = 0; j < n - 1 - i; j++) {
            if (nota[j] < nota[j+1]) {
                swap(nota[j], nota[j+1]);
                swap(nom[j], nom[j+1]);
            }
        }
    }

    for (int i = 0; i < 3 && i < n; i++)
        cout << nom[i] << " - " << nota[i] << "\n";
}
