#include <iostream>
#include <vector>
using namespace std;

int main() {
    int n;
    cin >> n;
    vector<double> v(n);

    for (int i = 0; i < n; i++) cin >> v[i];

    bool cap = true;
    int i = 0, j = n - 1;

    while (i < j) {
        if (v[i] != v[j]) {
            cap = false;
            break;
        }
        i++; j--;
    }

    if (cap) cout << "Es capicua";
    else cout << "No es capicua";
}
