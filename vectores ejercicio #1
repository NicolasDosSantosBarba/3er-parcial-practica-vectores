#include <iostream>
#include <vector>
using namespace std;

int main() {
    int n;
    cout << "Tamano: ";
    cin >> n;

    vector<double> v(n);
    cout << "Valores:\n";
    for (int i = 0; i < n; i++) cin >> v[i];

    double maximo = v[0];
    for (int i = 1; i < n; i++)
        if (v[i] > maximo)
            maximo = v[i];

    cout << "Maximo = " << maximo;
    return 0;
}
